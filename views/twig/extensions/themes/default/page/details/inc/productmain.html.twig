{% extends "page/details/inc/productmain.html.twig" %}

{% block details_productmain_social %}
    {{ parent() }}

    {% set recordDataJson = oView.getProduct()|record_data_json %}
    {% set useSidAsUserId = oViewConf.useSidAsUserId() %}

    <script>
        registerAddToCartListener({
            selector: '.js-oxProductForm',
            productData: JSON.parse('{{ recordDataJson|raw }}'),
            useSidAsUserId: {{ useSidAsUserId | json_encode() }}
        });
    </script>
{% endblock %}



